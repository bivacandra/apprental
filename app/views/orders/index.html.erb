<p id="notice"><%= notice %></p>

<h1>Checkout History</h1>

<table>
  <thead>
    <tr>
      <th>Order ID</th>
      <th>Car ID</th>
      <th>User ID</th>
      <th>Name</th>
      <th>TEL</th>
      <th>Pay Type</th>
      <th>Status</th>
      <th>Charge</th>
      <th>Guarantee</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
    <tr>
      <td><%= order.id %></td>
      <td><%= order.car_id %></td>
      <td><%= order.user_id %></td>
      <td><%= order.name %></td>
      <td><%= order.tel %></td>
      <td><%= order.pay_type %></td>
      <td><%= order.status %></td>
      <td><%= order.charge %></td>
      <td><%= order.guarantee %></td>
    </tr>
    <% end %>
  </tbody>
</table>