                                    <tr>
                                        <td class="h6"><strong>Car</strong></td>
                                        <td> </td>
                                        <td class="h5"><%= Car.find(order.car_id).name %></td>
                                    </tr>
                                     <tr>
                                        <td class="h6"><strong>Name</strong></td>
                                        <td> </td>
                                        <td class="h5"><%= order.name %></td>
                                    </tr>
                                    <tr>
                                        <td class="h6"><strong>Telphone</strong></td>
                                        <td> </td>
                                        <td class="h5"><%= order.tel %></td>
                                    </tr>
                                    <tr>
                                        <td class="h6"><strong>Payment Type</strong></td>
                                        <td> </td>
                                        <td class="h5"><%= order.pay_type %></td>
                                    </tr>
                                    <tr>
                                        <td class="h6"><strong>Status</strong></td>
                                        <td> </td>
                                        <td class="h5"><%= order.status %></td>
                                    </tr>
                                    <tr>
                                        <td class="h6"><strong>Charge</strong></td>
                                        <td> </td>
                                        <td class="h5"><%= order.charge %></td>
                                    </tr>
                                    <tr>
                                        <td class="h6"><strong>Guarantee</strong></td>
                                        <td> </td>
                                        <td class="h5"><%= order.guarantee %></td>
                                    </tr>
                                    
                                 </tbody>
                            </table>
                            <div class="modal-footer">
                                <h4 class="modal-title" id="myModalLabel"><button type="button" class="btn rent-btn" data-dismiss="modal">Close</button></h4>
                              </div>
                            
                        </div>
                      </div>
                  </div>
                </div></td>
            </tr>
            <% end %>
          </tbody>
        </table>
      </div>
      </div>
